body {
    overflow-x: scroll;
    overflow-y: scroll;
    font-family: Courier New;
    background-color: black;
    background-size:cover;
}
h1 {
    font-size: 50px;
}

h2 {
    font-size: 30px;
    font-family: Courier New;
    color:black;
}
ul {
    display: inline;
}

li {
    display: inline;
    list-style-type: none;
}



.toptitle {
   top: 0%;
   right:0%;
   font-family: Courier New;
   display: inline;
}
.headtool {
    position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    top: 0%;
    list-style-type: none;
    padding: 5px;
    background: none;
    font-family: Courier New;
    background-color: black;
    color: white;
}


.centerofPage {
    background: none;
    text-align:center;

}

.Bbutton {
    background-color: #008F4D;
    border: 5px outset;
    border-color: black;
    border-radius: 20px;
    padding: 7.5px 16px;
    font-size: 20px;
    cursor: pointer;
    font-family: Courier New;
    color: white;
    display: inline;
}
.smbutton {
    color: white;
    padding: 7.5px 16px;
    cursor: pointer;
    background: none;
    font-size: 20px;
    font-family: Courier New;
    background-color:black;
}
.facepic {
    background-color: darkcyan;
    border: 5px inset;
    border-color: darkgoldenrod;
    width: 300px;
    height: 300px;
  border-radius: 300px;
    color: white;
    font-size: 10px;
    font-family: 'Times New Roman';
    margin: 0 auto;
    text-align:center;
}
#Resume{
    display:inline-block;
}
.projects {
    text-align: center;
    border: 5px solid black;
    border-radius: 50px;
    background-color:white;
}
#Contact {
    display: inline-block;
}
#wrapper {
    margin-left: auto;
    margin-right: auto;
    color: white;
    background-color: #0047A6;
    
}
